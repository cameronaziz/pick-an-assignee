{
    "extends": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "i18n-text/no-en": "off",
      "import/no-namespace": "off",
      "no-unused-vars": "off",
      "object-curly-spacing": ["warn", "always"],
      "camelcase": "off",
      "semi": "off"
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
